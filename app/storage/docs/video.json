{
    "basePath": "http://video-lyrics.herokuapp.com/1.0",
    "swaggerVersion": "1.2",
    "apiVersion": "1.0",
    "resourcePath": "video",
    "apis": [
        {
            "path": "/video",
            "description": "Video look-up engine.",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Look-up a video.",
                    "nickname": "video",
                    "parameters": [
                        {
                            "name": "videoID",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "The ID of a video. If nothing is specified, a list containing all videos is returned."
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "A response containing an object with fields videoID (the ID of the video) and URL (the URL of the video). If nothing was found, then false will be returned."
                        },
                        {
                            "code": 500,
                            "message": "The query could not be executed."
                        }
                    ]
                }
            ]
        }
    ]
}